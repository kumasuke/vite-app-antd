{
  "name": "vite-app-antd",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "vite",
    "dev:api": "nodemon --watch ./src/api --ext ts --exec \"node --loader esbuild-register/loader -r esbuild-register ./src/api/server.ts\"",
    "dev": "pnpm run \"/^dev:.*/\"",
    "build:api": "esbuild --bundle --outfile=./.vercel/output/api/index.js --platform=node --target=node18 ./src/api/index.ts",
    "build:client": "tsc && vite build",
    "build": "pnpm run \"/^build:.*/\"",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.0",
    "antd": "^5.5.0",
    "nodemon": "^2.0.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.11.2",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vercel/node": "^2.14.2",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "esbuild": "^0.17.19",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-pages": "^0.29.1"
  }
}